#!/bin/bash

source buildrun.conf
cmake .. -DEXPS=$SS_EXPERIMENTS -DPERF=$SS_PERFTIMERS -DDEBUG=$SS_DEBUGLOGGER -DSYMBOLS=$SS_DEBUGSYMBOLS && make -j$SS_COMPILEWITHCORES &&  ../bin/$SS_EXECUTABLE -s $SS_SAMPLINGSCHEME -n $SS_TIMESEXECUTED --centralObj $SS_CENTRALOBJ $SS_EXPNAME $SS_BLENDFILES
# valgrind --leak-check=yes --undef-value-errors=no
